{
    "name": "vscode-eon",
    "displayName": "EON",
    "description": "Syntax highlighting for the EON configuration language with .eon file extension.",
    "version": "0.0.6",
    "publisher": "mh1",
    "repository": {
        "type": "git",
        "url": "https://github.com/matiashiltunen/vscode-eon"
    },
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "contributes": {
        "languages": [
            {
                "id": "eon",
                "aliases": [
                    "EON",
                    "eon"
                ],
                "extensions": [
                    ".eon"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "eon",
                "scopeName": "source.eon",
                "path": "./syntaxes/eon.tmLanguage.json"
            }
        ]
    }
}

